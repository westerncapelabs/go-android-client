MyScreenManager:
	EndState:


<EndState>:
	name: 'endstate'
	BoxLayout:
		orientation: 'vertical'
		pos_hint: {'center_x': 0.5, 'center_y': 0.5}
		size_hint: 1, 0.5
		spacing: '15dp'
		padding: '10dp'
		BoxLayout:
			orientation: 'horizontal'
			size_hint_y: None
			height: '48dp'
			padding: '5dp'

			canvas:
				Color:
					rgb: .3, .3, .3
				Rectangle:
					pos: self.pos
					size: self.size
			Label:
				id: end_state_label
				text: 'Thank you for registering!'
				font_size: '20sp'

		Button:
			text: 'FINISH'
			font_size: '18sp'
			pos_hint: {'center_x': 0.5, 'center_y': 0.5}
			size_hint: 0.5, 0.5
			size_hint_y: None

